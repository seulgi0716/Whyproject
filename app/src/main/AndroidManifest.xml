<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.whyproject">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/why_ic_launcher2"
        android:label="나한테 왜그래?!"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".GGang"
            android:hardwareAccelerated="true"
            android:parentActivityName=".RandomPicker" />
        <activity
            android:name=".GameRanking"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".CreatePassword" />
        <activity android:name=".PasswordActivity" />
        <activity
            android:name=".Chart"
            android:parentActivityName=".WhoteaseActivity" />
        <activity
            android:name=".Punch"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RandomPicker"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Trashcan"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".WhoteaseActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>